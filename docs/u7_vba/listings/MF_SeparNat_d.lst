' расчет натуральной сепарации газа на приеме насоса
Public Function MF_SeparNat_d( _
             ByVal Qliq_sm3day As Double, _
             ByVal fw_perc As Double, _
             ByVal Pin_atma As Double, _
    Optional ByVal Tin_C As Double = 50, _
